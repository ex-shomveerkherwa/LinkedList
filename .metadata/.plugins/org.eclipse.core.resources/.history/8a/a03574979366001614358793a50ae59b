package hashing;

public class BasicHash {
	
	static char[] input = {'s','i','d','d','h','u'};
	
	public void firstRepeatedCharacter(){
		int count[] = null;
		
		for(int i=0 ;i<256 ; i++) {
			count[i]=0;
		}
		
		for(int i=0 ;i< input.length; i++) {
			if(count[input[i]] == 1) {
				System.out.println("first repeated char "+input[i]);
			}
		}
	}

}
